"use strict";

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _react = _interopRequireDefault(require("react"));
var _reactNative = require("@testing-library/react-native");
var _Login = _interopRequireDefault(require("../src/components/Login"));
var _jsxRuntime = require("react/jsx-runtime");
describe("Login component", function () {
  test("renders correctly", function () {
    var _render = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Login.default, {})),
      getByTestId = _render.getByTestId;
    var loginScreen = getByTestId("login-screen");
    expect(loginScreen).toBeDefined();
  });
  test("email input changes value correctly", function () {
    var _render2 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Login.default, {})),
      getByPlaceholderText = _render2.getByPlaceholderText;
    var emailInput = getByPlaceholderText("Adresse email");
    _reactNative.fireEvent.changeText(emailInput, "test@example.com");
    expect(emailInput.props.value).toBe("test@example.com");
  });
  test("password input changes value correctly", function () {
    var _render3 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Login.default, {})),
      getByPlaceholderText = _render3.getByPlaceholderText;
    var passwordInput = getByPlaceholderText("Mot de passe top secret");
    _reactNative.fireEvent.changeText(passwordInput, "password123");
    expect(passwordInput.props.value).toBe("password123");
  });
  test("forgot password button navigates to correct screen", function () {
    var navigate = jest.fn();
    var _render4 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Login.default, {
        navigation: {
          navigate: navigate
        }
      })),
      getByTestId = _render4.getByTestId;
    var forgotPasswordButton = getByTestId("forgot-password-button");
    _reactNative.fireEvent.press(forgotPasswordButton);
    expect(navigate).toHaveBeenCalledWith("ForgetPassword");
  });
  test("signup button navigates to correct screen", function () {
    var navigate = jest.fn();
    var _render5 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(_Login.default, {
        navigation: {
          navigate: navigate
        }
      })),
      getByTestId = _render5.getByTestId;
    var signupButton = getByTestId("signup-button");
    _reactNative.fireEvent.press(signupButton);
    expect(navigate).toHaveBeenCalledWith("Signup");
  });
});
